/*
 * This application is comparing two integers
 */
package ComparingIntegers216;

/**
 *
 * @author ashleyyyyayyyy
 */
import java.util.Scanner; // import Scanner class

public class ComparingIntegers {
    // main method begins execution
    public static void main(String[] args) {
        // create Scanner to obtain input from command line
        Scanner input = new Scanner(System.in);
        
        // delcare two integers
        int number1;
        int number2;
        
        System.out.print("Enter first integer: ");
        number1 = input.nextInt(); // obtain integer from command line
        
        System.out.print("Enter second integer: ");
        number2 = input.nextInt(); // obtain integer from command line 
        
        // start comprison
        if (number1 > number2) {
            System.out.printf("%d is larger %n", number1);
        }
        if (number1 < number2) {
            System.out.printf("%d is larger %n", number2);
        }
        if (number1 == number2) {
            System.out.println("These numbers are equal");
        }
    } // end main method
} // end ComparingIntegers class
